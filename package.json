{
  "name": "dashboard-ui",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "set:env": "./scripts/checkversions.sh && apm install editorconfig language-javascript-jsx linter linter-eslint linter-stylint stylus language-marko",
    "preinstall": "[ '$NODE_ENV' != production ] && ./scripts/checkversions.sh",
    "pretest": "npm run lint:css && npm run lint:js",
    "lint:css": "./node_modules/.bin/stylint src/client/style/base/ --reporter stylint-stylish",
    "lint:js": "./node_modules/.bin/eslint ./src/** --max-warnings 5 --cache",
    "test": "npm run build:test && npm run test:mocha",
    "build:test": "node ./tests/scripts/prepare.js && ./node_modules/.bin/webpack --config ./tests/webpack.config.js",
    "test:mocha": "./node_modules/.bin/istanbul cover _mocha -- ./tests/bootstrap.js src/**/*.spec.js",
    "start:dev": "node src/server/",
    "start:tests": "node ./tests/server",
    "flow": "flow; test $? -eq 0 -o $? -eq 2"
  },
  "engines": {
    "node": "5.11.1"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "chart.js": "^1.1.1",
    "classnames": "^2.2.5",
    "config": "^1.21.0",
    "express": "^4.14.0",
    "marko": "^3.8.0",
    "normalize-css": "^2.3.1",
    "react": "^15.1.0",
    "react-chartjs": "^0.7.3",
    "react-dom": "^15.1.0",
    "react-router": "^2.5.1"
  },
  "devDependencies": {
    "babel-core": "^6.10.4",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-preset-stage-1": "^6.5.0",
    "chai": "^3.5.0",
    "chalk": "^1.1.3",
    "css-loader": "^0.23.1",
    "enzyme": "^2.4.1",
    "eslint": "^2.12.0",
    "eslint-plugin-react": "^5.1.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "flow-bin": "^0.29.0",
    "fs-extra": "^0.30.0",
    "gulp": "^3.9.1",
    "istanbul": "^1.1.0-alpha.1",
    "jsdom": "^9.4.1",
    "mocha": "^2.5.3",
    "react-addons-test-utils": "^15.2.1",
    "react-hot-loader": "^1.3.0",
    "style-loader": "^0.13.1",
    "stylint": "^1.3.10",
    "stylint-stylish": "^1.2.2",
    "stylus": "^0.54.5",
    "stylus-loader": "^2.1.1",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  }
}
